{
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "rules": {
        "no-console": 0,
        "max-len": 0,
        "no-plusplus": 0,
        "no-nested-ternary": 0,
        "import/prefer-default-export": 0,
        "no-underscore-dangle": 0,
        "arrow-parens": 0,
        "no-mixed-operators": 0
    },
    "plugins": [
        "import"
    ]
}